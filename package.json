{
  "name": "blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run gen-posts-config && craco start",
    "build": "npm run gen-posts-config && npm run gen-sitemap && craco build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "gen-sitemap": "node scripts/sitemap.js",
    "gen-posts-config": "node scripts/generatePostList.js && node scripts/generateClassList.js",
    "create-post": "node scripts/createPost.js",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,scss,json,md}\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^5.3.7",
    "@craco/craco": "^7.1.0",
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-brands-svg-icons": "^6.5.2",
    "@fortawesome/free-regular-svg-icons": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@lengineerc/utils": "^1.0.4",
    "@reduxjs/toolkit": "^2.2.6",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "antd": "^5.18.0",
    "axios": "^1.7.2",
    "echarts": "^5.5.0",
    "echarts-wordcloud": "^2.1.0",
    "framer-motion": "^12.23.24",
    "gray-matter": "^4.0.3",
    "highlight.js": "^11.9.0",
    "html-react-parser": "^5.2.10",
    "jinrishici": "^1.0.6",
    "markdown-navbar": "^1.4.3",
    "marked": "^13.0.0",
    "marked-highlight": "^2.1.2",
    "marked-katex-extension": "^5.0.2",
    "meting": "^2.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.23.1",
    "react-scripts": "5.0.1",
    "redux": "^5.0.1",
    "redux-saga": "^1.3.0",
    "sass": "^1.94.2",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@types/jest": "^27.5.2",
    "@types/markdown-navbar": "^1.4.4",
    "@types/node": "^16.18.98",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "gh-pages": "^6.2.0",
    "marked-katex-extension-ts": "^0.1.3",
    "prettier": "^3.6.2"
  }
}
